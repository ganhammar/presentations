{
  "name": "presentations",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "zx build.mjs",
    "build-slide": "slidev build presentations/${npm_config_name}/slides.md --out=${npm_config_out} --download --base=${npm_config_name}",
    "dev": "slidev --open presentations/${npm_config_name}/slides.md",
    "export": "slidev export presentations/${npm_config_name}/slides.md"
  },
  "dependencies": {
    "@slidev/cli": "^52.10.1",
    "@slidev/theme-apple-basic": "latest",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "playwright-chromium": "^1.57.0",
    "zx": "^8.8.5"
  }
}
